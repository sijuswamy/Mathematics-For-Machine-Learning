   "source": [
    "def fact(k):\n",
    "    k_ = 1\n",
    "    for i in range(1, k+1):\n",
    "        k_ *= i\n",
    "    return k_"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "accepting-dynamics",
   "metadata": {},
   "outputs": [],
   "source": [
    "def poisson_pmf(k, λ):\n",
    "    return np.exp(-λ)*λ**k/fact(k)"
   ]
