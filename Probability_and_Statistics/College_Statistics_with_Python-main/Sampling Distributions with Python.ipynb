   "cell_type": "code",
   "execution_count": 102,
   "id": "comic-ratio",
   "metadata": {},
   "outputs": [],
   "source": [
    "elements = np.arange(6)\n",
    "probabilities = [0.3, 0., 0.05, 0.05, 0.2, 0.4]\n",
    "X = np.random.choice(elements, 100, p=probabilities)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 103,
   "id": "informative-chemistry",
   "metadata": {},
